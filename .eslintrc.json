{
    "plugins": [
        "cypress"
      ],
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":  ["plugin:cypress/recommended", "airbnb-base"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error", 4],
        "no-return-assign": ["error", "except-parens"],
        // "no-param-reassign": ["error", { "props": true, "ignorePropertyModificationsFor": ["el.textContent"] }]
        "no-param-reassign": "off",
        "import/extensions": [0]
    }
}
